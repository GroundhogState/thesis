@article{Fendel:07,
author = {P. Fendel and S. D. Bergeson and Th. Udem and T. W. H\"{a}nsch},
journal = {Opt. Lett.},
keywords = {Coherent optical effects; Spectroscopy, high-resolution; Microsecond phenomena; Diode lasers; Frequency combs; Frequency measurement; Laser beams; Laser modes; Magnetic fields},
number = {6},
