@Article{Manning:10,
  author    = {A. G. Manning and S. S. Hodgman and R. G. Dall and M. T. Johnsson and A. G. Truscott},
  title     = {The Hanbury Brown-Twiss effect in a pulsed atom laser},
  journal   = {Opt. Express},
  year      = {2010},
  volume    = {18},
  number    = {18},
  pages     = {18712--18719},
  month     = {Aug},
  abstract  = {We have used the Hanbury Brown-Twiss effect to directly compare the density correlations of a pulsed atom laser and a pulsed ultracold thermal source of metastable helium. It was found that the isotropic RF outcoupling of atoms from a Bose-Einstein condensate does not result in decoherence, while the `bunching' typical of incoherent sources was observed for thermal atoms. This new method significantly increases data acquisition rates compared to previous measurements, and also permits future novel experiments which may allow us to probe processes such as the birth and death of a condensate by monitoring correlation effects.},
  doi       = {10.1364/OE.18.018712},
  keywords  = {Atomic and molecular physics; Atom optics ; Bose-Einstein condensates},
  publisher = {OSA},
  url       = {http://www.opticsexpress.org/abstract.cfm?URI=oe-18-18-18712},
}

