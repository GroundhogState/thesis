@article{Fendel:07,
author = {P. Fendel and S. D. Bergeson and Th. Udem and T. W. H\"{a}nsch},
journal = {Opt. Lett.},
keywords = {Coherent optical effects; Spectroscopy, high-resolution; Microsecond phenomena; Diode lasers; Frequency combs; Frequency measurement; Laser beams; Laser modes; Magnetic fields},
number = {6},
pages = {701--703},
publisher = {OSA},
title = {Two-photon frequency comb spectroscopy of the 6s-8s transition in cesium},
volume = {32},
month = {Mar},
year = {2007},
url = {http://ol.osa.org/abstract.cfm?URI=ol-32-6-701},
doi = {10.1364/OL.32.000701},
abstract = {We report a new absolute frequency measurement of the Cs 6s-8s two-photon transition measured using frequency comb spectroscopy. The fractional frequency uncertainty is 5{\texttimes}10{\textminus}11, a factor of 6 better than previous results. The comb is derived from a stabilized picosecond laser and referenced to an octave-spanning femtosecond frequency comb. The relative merits of picosecond-based frequency combs are discussed, and it is shown that the AC Stark shift of the transition is determined by the average rather than the much larger peak intensity.},
}

@article{Wu:13,
author = {Chien-Ming Wu and Tze-Wei Liu and Ming-Hsuan Wu and Ray-Kuang Lee and Wang-Yau Cheng},
journal = {Opt. Lett.},
keywords = {Metrology; Coherent optical effects; Linewidth; Spectroscopy, atomic; Diode lasers; Electrooptical modulators; Laser beams; Laser modes; Magnetic fields; Optical standards},
number = {16},
pages = {3186--3189},
publisher = {OSA},
title = {Absolute frequency of cesium 6{S}-8{S} 822 nm two-photon transition by a high-resolution scheme},
volume = {38},
month = {Aug},
year = {2013},
url = {http://ol.osa.org/abstract.cfm?URI=ol-38-16-3186},
doi = {10.1364/OL.38.003186},
abstract = {We present an alternative scheme for determining the frequencies of cesium (Cs) atom 6{S}\&\#x2013;8S Doppler-free transitions. With the use of a single electro-optical crystal, we simultaneously narrow the laser linewidth, lock the laser frequency, and resolve a narrow spectrum point by point. The error budget for this scheme is presented, and we prove that the transition frequency obtained from the Cs cell at room temperature and with one-layer \&\#x3BC;-metal shielding is already very near that for the condition of zero collision and zero magnetic field. We point out that a sophisticated linewidth measurement could be a good guidance for choosing a suitable Cs cell for better frequency accuracy.},
}

