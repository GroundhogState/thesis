@article {vanRooij196,
	author = {van Rooij, R. and Borbely, J. S. and Simonet, J. and Hoogerland, M. D. and Eikema, K. S. E. and Rozendaal, R. A. and Vassen, W.},
	title = {Frequency Metrology in Quantum Degenerate Helium: Direct Measurement of the $2^{3}{S}_{1}  \rightarrow 2^{1}{S}_{0}$ Transition},
	volume = {333},
	number = {6039},
	pages = {196--198},
	year = {2011},
	doi = {10.1126/science.1205163},
	publisher = {American Association for the Advancement of Science},
	abstract = {Precision spectroscopy of simple atomic systems has refined our understanding of the fundamental laws of quantum physics. In particular, helium spectroscopy has played a crucial role in describing two-electron interactions, determining the fine-structure constant and extracting the size of the helium nucleus. Here we present a measurement of the doubly forbidden 1557-nanometer transition connecting the two metastable states of helium (the lowest energy triplet state 2 3S1 and first excited singlet state 2 1S0), for which quantum electrodynamic and nuclear size effects are very strong. This transition is weaker by 14 orders of magnitude than the most predominantly measured transition in helium. Ultracold, submicrokelvin, fermionic 3He and bosonic 4He atoms are used to obtain a precision of 8 {\texttimes} 10-12, providing a stringent test of two-electron quantum electrodynamic theory and of nuclear few-body theory.},
	issn = {0036-8075},
	journal = {Science}
}

@article{Leeuwen_2006,
	doi = {10.1209/epl/i2006-10284-4},
	url = {https://doi.org/10.1209%2Fepl%2Fi2006-10284-4},
	year = 2006,
	month = {nov},
	publisher = {{IOP} Publishing},
	volume = {76},
	number = {3},
	pages = {409--415},
	author = {K. A. H. van Leeuwen and W Vassen},
	title = {Helium 23S-21Smetrology at 1.557 $\mu$m},
	journal = {Europhysics Letters ({EPL})},
	abstract = {An experiment is proposed to excite the “forbidden” 1s2s 3S1 - 1s2s 1S0 magnetic dipole (M1) transition at
1.557 μm in a collimated and slow atomic beam of
metastable helium atoms. It is demonstrated that an excitation
rate of 5000 −1 can be realised with the beam of a
2 W narrow-band telecom fiber laser intersecting the
atomic beam perpendicularly. A Doppler-limited sub-MHz 
spectroscopic linewidth is anticipated. Doppler-free
excitation of 2% of trapped and cooled atoms may be realised in 
a one-dimensional optical-lattice geometry, using the 2 W laser both
for trapping and spectroscopy. 
The very small (8 Hz) natural
linewidth of this transition presents an opportunity for accurate
tests of atomic structure
calculations of the helium atom. A measurement of the
3He-4He isotope shift allows for accurate determination of
the difference in nuclear charge radius of both isotopes.}
}


