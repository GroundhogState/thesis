@article{watson17,
  title = {Wavepackets in Inhomogeneous Periodic Media: Effective Particle-Field Dynamics and {{Berry}} Curvature},
  shorttitle = {Wavepackets in Inhomogeneous Periodic Media},
  author = {Watson, Alexander B. and Lu, Jianfeng and Weinstein, Michael I.},
  year = {2017},
  volume = {58},
  pages = {021503},
  issn = {0022-2488, 1089-7658},
  doi = {10.1063/1.4976200},
  abstract = {We consider a model of an electron in a crystal moving under the influence of an external electric field: Schr\textasciidieresis odinger's equation with a potential which is the sum of a periodic function and a general smooth function. We identify two dimensionless parameters: (re-scaled) Planck's constant and the ratio of the lattice spacing to the scale of variation of the external potential. We consider the special case where both parameters are equal and denote this parameter . In the limit {$\downarrow$} 0, we prove the existence of solutions known as semiclassical wavepackets which are asymptotic up to `Ehrenfest time' t {$\sim$} ln 1/ . To leading order, the center of mass and average quasi-momentum of these solutions evolve along trajectories generated by the classical Hamiltonian given by the sum of the Bloch band energy and the external potential. We then derive all corrections to the evolution of these observables proportional to . The corrections depend on the gauge-invariant Berry curvature of the Bloch band, and a coupling to the evolution of the wave-packet envelope which satisfies Schr\textasciidieresis odinger's equation with a time-dependent harmonic oscillator Hamiltonian. It is demonstrated that this coupling of observables (discrete particle-like degrees of freedom) to the wave-envelope (continuum field-like degrees of freedom) can have a very significant impact on the overall dynamics. We show that the coupled `particle-field' system for the evolution of the observables and wavepacket envelope may be derived from an `extended' -dependent Hamiltonian.},
  archiveprefix = {arXiv},
  eprint = {1611.03408},
  eprinttype = {arxiv},
  journal = {Journal of Mathematical Physics},
  keywords = {Condensed Matter - Mesoscale and Nanoscale Physics,Mathematical Physics,Mathematics - Analysis of PDEs,Quantum Physics},
  number = {2}
}

@article{weimer17,
