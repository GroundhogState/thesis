  urldate = {2020-05-08},
  abstract = {These lecture notes were created for a graduate-level course on quantum simulation taught at Leibniz University Hannover in 2013. The first part of the course discusses various state of the art methods for the numerical description of many-body quantum systems. In particular, I explain successful applications and inherent limitations due to the exponential complexity of the many-body problem. In the second part of the course, I show how using highly controllable quantum system such as ultracold atoms will provide a way to overcome the limitations of classical simulation methods. I discuss several theoretical and experimental achievements and outline the road for future developments.},
  archiveprefix = {arXiv},
  eprint = {1704.07260},
  eprinttype = {arxiv},
  journal = {arXiv:1704.07260 [cond-mat, physics:physics, physics:quant-ph]},
  keywords = {Condensed Matter - Quantum Gases,Condensed Matter - Statistical Mechanics,Physics - Atomic Physics,Quantum Physics},
  primaryclass = {cond-mat, physics:physics, physics:quant-ph}
}

@article{weiss16,
  title = {Higher-Order Quantum Bright Solitons in {{Bose}}-{{Einstein}} Condensates Show Truly Quantum Emergent Behavior},
  author = {Weiss, Christoph and Carr, Lincoln D.},
  year = {2016},
  url = {http://arxiv.org/abs/1612.05545},
  urldate = {2020-05-08},
  abstract = {When an interaction quench by a factor of four is applied to an attractive Bose-Einstein condensate, a higher-order quantum bright soliton exhibiting robust oscillations is predicted in the semiclassical limit by the Gross-Pitaevskii equation. Combining matrix-product state simulations of the Bose-Hubbard Hamiltonian with analytical treatment via the Lieb-Liniger model and the eigenstate thermalization hypothesis, we show these oscillations are absent. Instead, one obtains a large stationary soliton core with a small thermal cloud, a smoking-gun signal for non-semiclassical behavior on macroscopic scales and therefore a fully quantum emergent phenomenon.},
  archiveprefix = {arXiv},
  eprint = {1612.05545},
  eprinttype = {arxiv},
  journal = {arXiv:1612.05545 [cond-mat, physics:nlin, physics:quant-ph]},
  keywords = {Condensed Matter - Quantum Gases,Nonlinear Sciences - Pattern Formation and Solitons,Quantum Physics},
  primaryclass = {cond-mat, physics:nlin, physics:quant-ph}
}

