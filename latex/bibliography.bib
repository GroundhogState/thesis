@phdthesis{ManningThesis,
  title = {Foundation experiments in quantum atom optics with ultracold metastable helium},
  abstract = {The ﬁeld of atom optics has progressed rapidly over the past 20 years since the realisation of Bose-Einstein condensation, such that the wave behaviour of atomic gases is now routinely demonstrated. Furthermore, the study of quantum atom optics, which goes beyond a ‘mean-ﬁeld’ description of quantum systems to consider behaviour of single particles, has demonstrated both the similarities between photons and massive species, and the differences between the two as a result of the internal structure and external interactions of atoms. An important class of quantities which allow such effects to be measured are nth order correlation functions, which can be interpreted as a result of either particle or wave behaviour, and provide a statistical description of ﬂuctuations in n-tuples of particles in a source, which rigorously deﬁnes concepts such as coherence. The quantum statistics of a Bose-Einstein condensate should be the same as that for an optical laser, while an ideal thermal Bose gas matches the behaviour of an incoherent source of light. However, correlation measurements can also be used to quantify the inﬂuence of interactions, dimensionality, conﬁning potentials and waveguides, and the difference in quantum statistics between fermions and bosons, which illustrates the rich range of behaviour exhibited by atomic gases.},
  language = {en},
  author = {Manning, Andrew Geoffrey},
  pages = {193},
  file = {Manning - Foundation experiments in quantum atom optics with.pdf:C\:\\Users\\jaker\\Zotero\\storage\\MPHYMHUS\\Manning - Foundation experiments in quantum atom optics with.pdf:application/pdf}
}

