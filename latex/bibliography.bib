@article{Drake88,
title = {High precision variational calculations for the 1s21S state of H − and the 1s21s, 1s2s 1s and 1s2s 3s states of helium},
journal = {Nuclear Instruments and Methods in Physics Research Section B: Beam Interactions with Materials and Atoms},
volume = {31},
