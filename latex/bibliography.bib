@article{Fletcher17,
  title ={Two- and three-body contacts in the unitary Bose gas},
  volume ={355},
  issn ={0036-8075, 1095-9203},
  url ={https://www.sciencemag.org/lookup/doi/10.1126/science.aai8195},
  doi ={10.1126/science.aai8195},
  abstract ={In many-body systems governed by pairwise contact interactions, a wide range of observables is linked by a single parameter, the two-body contact, which quantifies two-particle correlations. This profound insight has transformed our understanding of strongly interacting Fermi gases. Using Ramsey interferometry, we studied coherent evolution of the resonantly interacting Bose gas, and we show here that it cannot be explained by only pairwise correlations. Our experiments reveal the crucial role of three-body correlations arising from Efimov physics and provide a direct measurement of the associated three-body contact.},
  
  number ={6323},
  
